<Application x:Class="DccPushAlarmTester.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DccPushAlarmTester"
             xmlns:localCore="clr-namespace:DccPushAlarmTester.Core"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/DMI.Common;component/ResourceDictionary/ColorResourceDictionary.xaml"></ResourceDictionary>
                <ResourceDictionary Source="pack://application:,,,/DMI.Common;component/ResourceDictionary/ToolDictionary.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
            <localCore:ViewModelLocator x:Key="ViewModelLocator"/>
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
